import java.util.Scanner;

public class TestUserArrayInput 
{
	
	public static void Arr(int[] A)
	{
		A = new int[10];
		System.out.println("Enter your ints");
		Scanner sc = new Scanner(System.in);
		for(int i = 0; i < 10; i++)
		{
			A[i] = sc.nextInt();
		}
		
	}
	public static void main(String[] args) {
		Arr();
		
}
